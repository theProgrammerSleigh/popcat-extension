{
    "name": "Sonic PopCat",
    "description": "The Quickest Automated Tool to Rise Up in the PopCat Leaderboards",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://popcat.click/*"],
            "css": ["style.css"],
            "js": ["pop.js"]
        }
    ],
    "icons": {
        "48": "images/sonicpop-48.png",
        "128": "images/sonicpop-128.png"
    },
    "action": {
        "default_icon": {
            "48": "images/sonicpop-48.png",
            "128": "images/sonicpop-128.png"
        },
        "default_title": "Sonic PopCat"
    }
  }